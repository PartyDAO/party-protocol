    function testTokenURI() public {
        // Create party
        DummyParty party = new DummyParty(address(globals), "Party of the Living Dead");

        // Create proposals
        uint256 n = _randomRange(0, 6);
        for (uint256 i; i < n; ++i) {
            _createMockProposal(party);
        }

        bool hasClaimed = n > 3;
